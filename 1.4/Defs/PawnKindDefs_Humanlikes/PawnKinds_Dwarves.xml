<?xml version="1.0" encoding="utf-8"?>
<Defs>

      <PawnKindDef Name="StonebornBase" Abstract="True">
        <race>Human</race>
        <defaultFactionType>OutlanderRoughStoneborn</defaultFactionType>
        <invNutrition>3.2</invNutrition>
        <chemicalAddictionChance>0.5</chemicalAddictionChance>
        <combatEnhancingDrugsChance>0</combatEnhancingDrugsChance>	
        <xenotypeSet>
          <xenotypeChances>
            <Stoneborn>999</Stoneborn>
          </xenotypeChances>
        </xenotypeSet>
      </PawnKindDef>
    
      <PawnKindDef ParentName="SlaveBase">
        <defName>Slave_Stoneborn</defName>
        <xenotypeSet Inherit="False">
          <xenotypeChances>
            <Gnoblin MayRequire="VexedTrees980.RimGnoblinsBiotech">0.50</Gnoblin>	
            <Neanderthal>0.125</Neanderthal>
            <Yttakin>0.125</Yttakin>
            <Hussar>0.125</Hussar>
          </xenotypeChances>
        </xenotypeSet>
      </PawnKindDef>
    
      <!--===================== Basic Dwarves =========================-->
    
      <PawnKindDef ParentName="StonebornBase">
        <defName>Stoneborn_Worker</defName>
        <label>stoneborn worker</label>
        <maxGenerationAge>75</maxGenerationAge>	
        <combatPower>30</combatPower>
        <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
        <forceNormalGearQuality>true</forceNormalGearQuality>
        <isFighter>false</isFighter>
        <apparelTags>
          <li>IndustrialBasic</li>
        </apparelTags>
        <backstoryFiltersOverride>
          <li>
            <categories>
              <li>Offworld</li>
              <li>Outlander</li>
              <li>ImperialCommon</li>
            </categories>
          </li>
        </backstoryFiltersOverride>		
        <apparelMoney>200~800</apparelMoney>
        <gearHealthRange>1~1</gearHealthRange>
        <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
        <techHediffsMoney>100~1000</techHediffsMoney>
        <techHediffsTags>
          <li>Poor</li>
          <li>Simple</li>
        </techHediffsTags>
        <techHediffsChance>0.1</techHediffsChance>
        <apparelDisallowTags>
          <li>Western</li>
        </apparelDisallowTags>
        <disallowedTraits>
          <li>Nudist</li>
        </disallowedTraits>
        <initialWillRange>2~3</initialWillRange>
        <initialResistanceRange>20~30</initialResistanceRange>
      </PawnKindDef>
    
      <!--===================== Elven fighter castes =========================-->
    
      <PawnKindDef Name="StonebornWarriorBase" ParentName="StonebornBase" Abstract="True">
        <race>Human</race>
        <maxGenerationAge>55</maxGenerationAge>
        <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
        <backstoryFiltersOverride>
          <li>
            <categories>
              <li>ImperialFighter</li>
            </categories>
          </li>
        </backstoryFiltersOverride>
        <styleItemTags>
          <li>
            <tag>Soldier</tag>
            <baseWeight>1</baseWeight>
            <weightFactor>1</weightFactor>
          </li>
        </styleItemTags>
        <invNutrition>3</invNutrition>
        <itemQuality>Good</itemQuality>
        <weaponMoney>1300~2500</weaponMoney>
        <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
        <apparelMoney>1250~2500</apparelMoney>
        <apparelTags>
          <li>IndustrialBasic</li>
          <li>IndustrialAdvanced</li>
          <li>IndustrialMilitaryBasic</li>
          <li>IndustrialMilitaryAdvanced</li>
        </apparelTags>
        <apparelDisallowTags>
          <li>Western</li>
        </apparelDisallowTags>
        <specificApparelRequirements>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Cloth</stuff>
          </li>
          <li>
            <bodyPartGroup>Legs</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Cloth</stuff>
          </li>
        </specificApparelRequirements>
        <techHediffsChance>0.35</techHediffsChance>
        <techHediffsMoney>1000~1500</techHediffsMoney>
        <biocodeWeaponChance>0.3</biocodeWeaponChance>
      </PawnKindDef>
    
      <!-- ranger -->
    
      <PawnKindDef Name="StonebornSoldierBase" Abstract="True" ParentName="StonebornWarriorBase">
        <combatPower>65</combatPower>
        <apparelMoney>5000~8000</apparelMoney>
        <weaponTags>
          <li>IndustrialGunAdvanced</li>
        </weaponTags>
        <apparelRequired>
          <li>Apparel_FlakJacket</li>
          <li>Apparel_FlakVest</li>
          <li>Apparel_FlakPants</li>
        </apparelRequired>
        <initialWillRange>3~4</initialWillRange>
      </PawnKindDef>
    
      <PawnKindDef ParentName="StonebornSoldierBase">
        <defName>Stoneborn_Ranger</defName>
        <label>ranger</label>
        <apparelColor>(101,87,60)</apparelColor>
        <specificApparelRequirements>
          <li>
            <bodyPartGroup>UpperHead</bodyPartGroup>
            <alternateTagChoices>
              <li>
                <tag>DwarfHelmetRanger</tag>
                <chance>0.25</chance>
              </li>
            </alternateTagChoices>
          </li>
        </specificApparelRequirements>
        <techHediffsTags>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <skills>
          <li>
            <skill>Shooting</skill>
            <range>4~10</range>
          </li>
        </skills>
        <disallowedTraits>
          <li>Brawler</li>
        </disallowedTraits>
        <initialResistanceRange>15~24</initialResistanceRange>
      </PawnKindDef>
    
      <!-- Firedrake -->
    
      <PawnKindDef ParentName="StonebornSoldierBase">
        <defName>Stoneborn_Firedrake</defName>
        <label>firedrake</label>
        <apparelColor>(68,68,68)</apparelColor>
        <canBeSapper>false</canBeSapper>
        <isGoodBreacher>false</isGoodBreacher>
        <combatPower>220</combatPower>
        <weaponTags Inherit="False">
          <li>Flameblaster</li>
        </weaponTags>
        <apparelMoney>6000~9000</apparelMoney>
        <apparelRequired Inherit="False">
          <li>DV_Apparel_BeardedHelmet</li>
          <li>Apparel_PowerArmor</li>
        </apparelRequired>
        <specificApparelRequirements Inherit="False">
          <li>
            <bodyPartGroup>FullHead</bodyPartGroup>
            <color>(156,69,31)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Devilstrand</stuff>
          </li>
          <li>
            <bodyPartGroup>Legs</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Devilstrand</stuff>
          </li>
        </specificApparelRequirements>
        <techHediffsTags>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <skills>
          <li>
            <skill>Shooting</skill>
            <range>6~12</range>
          </li>
        </skills>
        <disallowedTraits>
          <li>Brawler</li>
        </disallowedTraits>
        <initialWillRange>2~4</initialWillRange>
        <initialResistanceRange>20~25</initialResistanceRange>
      </PawnKindDef>
    
      <!-- Thunderer -->
    
      <PawnKindDef Name="StonebornKnightBase" Abstract="True" ParentName="StonebornSoldierBase">
        <combatPower>100</combatPower>
        <weaponMoney>1550~2700</weaponMoney>
        <weaponTags>
          <li>IndustrialGunAdvanced</li>
          <li>SpacerGun</li>
          <li>SniperRifle</li>
        </weaponTags>
        <apparelMoney>6000~9000</apparelMoney>
        <apparelRequired Inherit="False">
          <li>DV_Apparel_GildedDwarfHelm</li>
          <li>Apparel_Bandolier</li>
          <li>DV_Apparel_IronbreakerArmor</li>
        </apparelRequired>
        <disallowedTraits>
          <li>Brawler</li>
          <li>Wimp</li>
          <li>Nudist</li>
          <li>BodyPurist</li>
        </disallowedTraits>
      </PawnKindDef>
    
      <PawnKindDef ParentName="StonebornKnightBase">
        <defName>Stoneborn_Thunderer</defName>
        <label>thunderer</label>
        <techHediffsTags>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <specificApparelRequirements Inherit="False">
          <li>
            <color>(90,90,90)</color>
            <alternateTagChoices>
              <li>
                <tag>Gunlink</tag>
                <chance>0.3</chance>
              </li>
              <li>
                <tag>GildedDwarfHelmet</tag>
                <chance>0.8</chance>
              </li>
            </alternateTagChoices>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>Shell</apparelLayer>
            <requiredTag>Cape</requiredTag>
            <color>(108, 78, 55)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>Middle</apparelLayer>
            <requiredTag>DwarfArmor</requiredTag>
            <color>(60,60,60)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Cloth</stuff>
          </li>
          <li>
            <bodyPartGroup>Legs</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <stuff>Cloth</stuff>
          </li>
        </specificApparelRequirements>
        <skills>
          <li>
            <skill>Shooting</skill>
            <range>9~15</range>
          </li>
        </skills>
        <initialWillRange>3~5</initialWillRange>
        <initialResistanceRange>13~21</initialResistanceRange>
      </PawnKindDef>
    
      <!-- Slayer -->
    
      <PawnKindDef ParentName="StonebornSoldierBase">
        <defName>Stoneborn_Slayer</defName>
        <label>slayer</label>
        <maxGenerationAge>50</maxGenerationAge>
        <combatPower>120</combatPower>
        <weaponTags Inherit="False">
          <li>DwarfMeleeAdvanced</li>
          <li>UltratechMelee</li>
        </weaponTags>
        <apparelRequired Inherit="False">
          <li>Apparel_PlateArmor</li>
          <li>Apparel_ShieldBelt</li>
        </apparelRequired>
        <apparelColor>(133, 105, 51)</apparelColor>
        <techHediffsTags>
          <li>Advanced</li>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <skills>
          <li>
            <skill>Melee</skill>
            <range>9~15</range>
          </li>
        </skills>
        <initialWillRange>3~5</initialWillRange>
        <initialResistanceRange>25~30</initialResistanceRange>
      </PawnKindDef>
    
      <!-- Royals -->
    
      <PawnKindDef Name="NobilityStonebornBase" Abstract="True">
        <race>Human</race>
        <defaultFactionType>OutlanderRoughStoneborn</defaultFactionType>
        <maxGenerationAge>120</maxGenerationAge>
        <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
        <backstoryFiltersOverride>
          <li>
            <categories>
              <li>ImperialRoyal</li>
            </categories>
          </li>
        </backstoryFiltersOverride>
        <royalTitleChance>1</royalTitleChance>
        <chemicalAddictionChance>0.05</chemicalAddictionChance>
        <itemQuality>Excellent</itemQuality>
        <combatPower>50</combatPower>
        <weaponMoney>2600~3100</weaponMoney>
        <weaponTags>
          <li>UltratechMelee</li>
          <li>Bladelink</li>
          <li>DwarfMeleeRoyal</li>
          <li>DwarfRangedRoyal</li>
        </weaponTags>
        <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
        <apparelMoney>1000~1500</apparelMoney>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <apparelTags>
          <li>IndustrialBasic</li>
        </apparelTags>
        <apparelDisallowTags>
          <li>Western</li>
        </apparelDisallowTags>
        <techHediffsChance>0.45</techHediffsChance>
        <techHediffsMoney>2000~4000</techHediffsMoney>
        <techHediffsTags>
          <li>Advanced</li>
          <li>ImplantEmpireRoyal</li>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <techHediffsDisallowTags>
          <li>PainCauser</li>
        </techHediffsDisallowTags>
        <styleItemTags>
          <li>
            <tag>Royalty</tag>
            <baseWeight>1</baseWeight>
            <weightFactor>1</weightFactor>
          </li>
        </styleItemTags>
        <disallowedTraits>
          <li>Nudist</li>
          <li>BodyPurist</li>
          <li>Brawler</li>
          <li>Wimp</li>
        </disallowedTraits>
      </PawnKindDef>

      <PawnKindDef ParentName="NobilityStonebornBase">
        <defName>Stoneborn_Underlord</defName>
        <label>underlord</label>
        <factionLeader>true</factionLeader>
        <weaponTags>
          <li>UltratechMelee</li>
          <li>Bladelink</li>
          <li>DwarfMeleeRoyal</li>
          <li>DwarfRangedRoyal</li>
        </weaponTags>
        <specificApparelRequirements>
          <li>
            <bodyPartGroup>UpperHead</bodyPartGroup>
            <requiredTag>DwarfRoyalTier2</requiredTag>
            <color>(138, 51, 36)</color>
            <stuff>Gold</stuff>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>Shell</apparelLayer>
            <requiredTag>DwarfRoyal</requiredTag>
            <color>(108, 78, 55)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>Middle</apparelLayer>
            <requiredTag>RoyalTier5</requiredTag>
            <color>(138, 51, 36)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <apparelLayer>OnSkin</apparelLayer>
            <requiredTag>RoyalTier2</requiredTag>
            <color>(138, 51, 36)</color>
          </li>
          <li>
            <bodyPartGroup>Torso</bodyPartGroup>
            <stuff>Hyperweave</stuff>
          </li>
          <li>
            <bodyPartGroup>Legs</bodyPartGroup>
            <stuff>Hyperweave</stuff>
          </li>
        </specificApparelRequirements>
        <techHediffsTags>
          <li>Advanced</li>
          <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <initialWillRange>3~5</initialWillRange>
        <initialResistanceRange>25~30</initialResistanceRange>
      </PawnKindDef>



</Defs>